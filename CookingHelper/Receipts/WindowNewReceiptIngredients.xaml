<Window x:Class="CookingHelper.WindowNewReceiptIngredients"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookingHelper"
        mc:Ignorable="d"
        Title="Zutatenart auswählen" Height="300" Width="300">
    <Grid Background="#FF323232">
        <ListBox x:Name="ListBoxIngredientType" HorizontalAlignment="Left" Height="168" Margin="59,35,0,0" VerticalAlignment="Top" Width="168" KeyDown="ListBoxIngredientType_KeyDown" MouseDoubleClick="ListBoxIngredientType_MouseDoubleClick"/>
        <Button x:Name="CmdClose" Content="Abbrechen" HorizontalAlignment="Left" Margin="193,233,0,0" VerticalAlignment="Top" Width="75" Click="CmdClose_Click"/>
        <Button x:Name="CmdChoose" Content="Auswählen" HorizontalAlignment="Left" Margin="23,233,0,0" VerticalAlignment="Top" Width="75" Click="CmdChoose_Click"/>

    </Grid>
</Window>
